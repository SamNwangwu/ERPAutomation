          },
          "Deploy_VNet_and_Peering": {
            "inputs": {
              "body": {
                "properties": {
                  "mode": "Incremental",
                  "parameters": {
                    "addressPrefix": {
                      "value": "@triggerBody()?['Allocated_x0020_IP_x0020_Address']"
                    },
                    "hubVnetId": {
                      "value": "/subscriptions/8cc0a954-47fb-4878-a768-e195dbd036b7/resourceGroups/rg-lebara-security-hub-northeurope/providers/Microsoft.Network/virtualNetworks/vnet-lebara-security-hub"
                    },
                    "location": {
                      "value": "North Europe"
                    },
                    "vnetName": {
                      "value": "@triggerBody()?['VNet_x0020_Name']"
                    }
                  },
                  "templateLink": {
                    "uri": "https://erpautomationtemplates.blob.core.windows.net/arm-templates/vnet-deployment.json"
                  }
                }
              },
              "host": {
                "connection": {
                  "name": "@parameters('$connections')['arm-2']['connectionId']"
                }
              },
              "method": "put",
              "path": "/subscriptions/@{encodeURIComponent('8cc0a954-47fb-4878-a768-e195dbd036b7')}/resourcegroups/@{encodeURIComponent('rg-lebara-d365-networking')}/providers/Microsoft.Resources/deployments/@{encodeURIComponent(concat('deploy-', utcNow('yyyyMMddHHmmss')))}",
              "queries": {
                "wait": true,
                "x-ms-api-version": "2016-06-01"
              }
            },
            "type": "ApiConnection"
          }
        },
        "else": {
          "actions": {}
        },
        "expression": {
