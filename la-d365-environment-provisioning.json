  "definition": {
    "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
    "actions": {
      "Condition_1_(Status_=_Approved_AND_Deployment_Status_=_Ready_for_Deployment)": {
        "actions": {
          "Deploy_Hub_to_Spoke_Peering": {
            "inputs": {
              "body": {
                "properties": {
                  "mode": "Incremental",
                  "parameters": {
                    "hubVnetName": {
                      "value": "vnet-lebara-security-hub"
                    },
                    "peeringName": {
                      "value": "hub-to-@{triggerBody()?['VNet_x0020_Name']}"
                    },
                    "spokeVnetId": {
                      "value": "@{body('Deploy_VNet_and_Peering')?['properties']?['outputs']?['vnetResourceId']?['value']}"
                    }
                  },
                  "template": {
                    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
                    "contentVersion": "1.0.0.0",
                    "parameters": {
                      "hubVnetName": {
                        "type": "string"
                      },
                      "peeringName": {
                        "type": "string"
                      },
                      "spokeVnetId": {
                        "type": "string"
                      }
                    },
                    "resources": [
                      {
                        "apiVersion": "2024-01-01",
                        "name": "[concat(parameters('hubVnetName'), '/', parameters('peeringName'))]",
                        "properties": {
                          "allowForwardedTraffic": true,
                          "allowGatewayTransit": true,
                          "allowVirtualNetworkAccess": true,
                          "remoteVirtualNetwork": {
                            "id": "[parameters('spokeVnetId')]"
                          },
                          "useRemoteGateways": false
                        },
                        "type": "Microsoft.Network/virtualNetworks/virtualNetworkPeerings"
                      }
                    ]
                  }
                }
              },
              "host": {
                "connection": {
                  "name": "@parameters('$connections')['arm-2']['connectionId']"
                }
              },
              "method": "put",
              "path": "/subscriptions/@{encodeURIComponent('8cc0a954-47fb-4878-a768-e195dbd036b7')}/resourcegroups/@{encodeURIComponent('rg-lebara-security-hub-northeurope')}/providers/Microsoft.Resources/deployments/@{encodeURIComponent('hub-to-spoke-peering')}",
              "queries": {
                "wait": true,
                "x-ms-api-version": "2016-06-01"
              }
            },
            "runAfter": {
              "Deploy_VNet_and_Peering": [
                "Succeeded"
              ]
            },
            "type": "ApiConnection"
          },
          "Deploy_NSG_and_Associate": {
            "inputs": {
              "body": {
                "properties": {
