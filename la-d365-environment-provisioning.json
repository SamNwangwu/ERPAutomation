                      "value": "@triggerBody()?['Allocated_x0020_IP_x0020_Address']"
                    },
                    "vnetName": {
                      "value": "@triggerBody()?['VNet_x0020_Name']"
                    }
                  },
                  "template": {
                    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
                    "contentVersion": "1.0.0.0",
                    "parameters": {
                      "addressPrefix": {
                        "type": "string"
                      },
                      "location": {
                        "defaultValue": "northeurope",
                        "type": "string"
                      },
                      "vnetName": {
                        "type": "string"
                      }
                    },
                    "resources": [
                      {
                        "apiVersion": "2024-01-01",
                        "location": "[parameters('location')]",
                        "name": "[variables('nsgName')]",
                        "properties": {
                          "securityRules": [
                            {
                              "name": "AllowHTTPS-LCS",
                              "properties": {
                                "access": "Allow",
                                "description": "Allow HTTPS from LCS for deployment monitoring and configuration",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "443",
                                "direction": "Inbound",
                                "priority": 110,
                                "protocol": "Tcp",
                                "sourceAddressPrefixes": [
                                  "191.239.20.104",
                                  "40.76.5.241",
                                  "40.112.209.123",
                                  "40.121.208.21",
                                  "40.118.145.241",
                                  "20.253.188.249",
                                  "172.191.225.240"
                                ],
                                "sourcePortRange": "*"
                              }
                            },
                            {
                              "name": "AllowWinRM-MS-1",
                              "properties": {
                                "access": "Allow",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "5986",
                                "direction": "Inbound",
                                "priority": 120,
                                "protocol": "Tcp",
                                "sourceAddressPrefix": "191.239.20.104",
                                "sourcePortRange": "*"
                              }
                            },
                            {
                              "name": "AllowWinRM-MS-2",
                              "properties": {
                                "access": "Allow",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "5986",
                                "direction": "Inbound",
                                "priority": 121,
                                "protocol": "Tcp",
                                "sourceAddressPrefix": "40.76.5.241",
                                "sourcePortRange": "*"
                              }
                            },
                            {
                              "name": "AllowWinRM-MS-3",
                              "properties": {
                                "access": "Allow",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "5986",
                                "direction": "Inbound",
                                "priority": 122,
                                "protocol": "Tcp",
                                "sourceAddressPrefix": "40.112.209.123",
                                "sourcePortRange": "*"
                              }
                            },
                            {
                              "name": "AllowWinRM-MS-4",
                              "properties": {
                                "access": "Allow",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "5986",
                                "direction": "Inbound",
                                "priority": 123,
                                "protocol": "Tcp",
                                "sourceAddressPrefix": "40.121.208.21",
                                "sourcePortRange": "*"
                              }
                            },
                            {
                              "name": "AllowWinRM-MS-5",
                              "properties": {
                                "access": "Allow",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "5986",
                                "direction": "Inbound",
                                "priority": 124,
                                "protocol": "Tcp",
                                "sourceAddressPrefix": "40.118.145.241",
                                "sourcePortRange": "*"
                              }
                            },
                            {
                              "name": "AllowWinRM-MS-6",
                              "properties": {
                                "access": "Allow",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "5986",
                                "direction": "Inbound",
                                "priority": 125,
                                "protocol": "Tcp",
                                "sourceAddressPrefix": "20.253.188.249",
                                "sourcePortRange": "*"
                              }
                            },
                            {
                              "name": "AllowWinRM-MS-7",
                              "properties": {
                                "access": "Allow",
                                "destinationAddressPrefix": "*",
                                "destinationPortRange": "5986",
                                "direction": "Inbound",
                                "priority": 126,
                                "protocol": "Tcp",
                                "sourceAddressPrefix": "172.191.225.240",
                                "sourcePortRange": "*"
                              }
                            }
                          ]
                        },
                        "type": "Microsoft.Network/networkSecurityGroups"
                      },
                      {
                        "apiVersion": "2024-01-01",
                        "dependsOn": [
                          "[resourceId('Microsoft.Network/networkSecurityGroups', variables('nsgName'))]"
                        ],
                        "name": "[concat(parameters('vnetName'), '/', variables('subnetName'))]",
                        "properties": {
                          "addressPrefix": "[parameters('addressPrefix')]",
                          "networkSecurityGroup": {
                            "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('nsgName'))]"
                          }
                        },
                        "type": "Microsoft.Network/virtualNetworks/subnets"
                      }
                    ],
                    "variables": {
                      "nsgName": "[concat('nsg-', parameters('vnetName'))]",
                      "subnetName": "default"
                    }
                  }
                }
              },
              "host": {
                "connection": {
                  "name": "@parameters('$connections')['arm-2']['connectionId']"
                }
              },
              "method": "put",
              "path": "/subscriptions/@{encodeURIComponent('8cc0a954-47fb-4878-a768-e195dbd036b7')}/resourcegroups/@{encodeURIComponent('rg-lebara-d365-networking')}/providers/Microsoft.Resources/deployments/@{encodeURIComponent(concat('nsg-deploy-', utcNow('yyyyMMddHHmmss')))}",
              "queries": {
                "wait": true,
                "x-ms-api-version": "2016-06-01"
              }
            },
